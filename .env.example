# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Google Docs Configuration (Option A)
# Follow setup instructions in README.md to get these credentials
GOOGLE_OAUTH_CLIENT_SECRETS=credentials/oauth_client_secrets.json
GOOGLE_DOC_FOLDER_ID=your-google-drive-folder-id

# Chorus API Configuration (Optional - for live data)
CHORUS_API_KEY=your-chorus-api-key
CHORUS_API_URL=https://api.chorus.ai/v1

# Web Dashboard Configuration (Option B)
WEB_HOST=127.0.0.1
WEB_PORT=8000

# Model Configuration
OPENAI_MODEL=gpt-4.1-mini

# Processing Configuration
MAX_INSIGHTS_PER_CATEGORY=10
MIN_INSIGHTS_PER_CATEGORY=3
ENABLE_DEDUPLICATION=true
CONFIDENCE_THRESHOLD=medium

# LangSmith Tracing (Optional - for observability and debugging)
# Get your API key from https://smith.langchain.com/
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langsmith-api-key-here
LANGCHAIN_PROJECT=meet-insights
# Optional: Set to "true" to enable detailed tracing
LANGCHAIN_VERBOSE=false
